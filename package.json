{
  "name": "vue-cli-plugin-test-attrs",
  "version": "0.1.5",
  "author": {
    "name": "Thorsten LÃ¼nborg",
    "url": "https://github.com/linusborg"
  },
  "repository": {
    "url": "https://github.com/linusborg/vue-cli-plugin-test-attrs"
  },
  "private": false,
  "main": "index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "serve:on": "vue-cli-service serve ./example/main.js",
