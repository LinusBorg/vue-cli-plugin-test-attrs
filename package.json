    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:w": "vue-cli-service test:unit --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
