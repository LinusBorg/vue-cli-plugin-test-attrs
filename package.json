    "@vue/test-utils": "^1.0.0-beta.25",
    "lint-staged": "^7.2.0",
    "memfs": "^2.10.1",
    "prettier-eslint": "^8.8.2",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "vuePlugins": {
    "service": [
      "./index.js"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
